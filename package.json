{
	"name": "express-api-starter-ts",
	"version": "1.2.0",
	"description": " A basic starter for an express.js API with Typescript",
	"main": "src/index.ts",
	"scripts": {
		"start": "npm run build && npx concurrently \"node dist/src/index.js\" \"node dist/src/api/event-getter/worker/worker.js\"",
		"worker": "nodemon src/api/event-getter/worker/worker.ts",
		"dev": "nodemon src/index.ts",
		"servers": "npx concurrently \"nodemon src/index.ts\" \"nodemon src/api/event-getter/worker/worker.ts\"",
		"build": "tsc",
		"start:dist": "node dist/src/index.js",
		"lint": "eslint --fix src test",
		"test": "jest",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [],
	"dependencies": {
		"@anthropic-ai/sdk": "^0.57.0",
		"@bull-board/express": "^6.12.0",
		"@prisma/client": "^6.7.0",
		"anthropic-ai": "^0.0.10",
		"bullmq": "^5.56.8",
		"cheerio": "^1.1.2",
		"cli-table3": "^0.6.5",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.21.1",
		"helmet": "^8.0.0",
		"morgan": "^1.10.0",
		"node-cron": "^4.2.1",
		"redis": "^5.6.1",
		"zod": "^3.24.4",
		"zod-validation-error": "^3.4.0"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "^2.8.19",
		"@types/express": "^4.17.23",
		"@types/jest": "^29.5.13",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.7.5",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^7.16.1",
		"@typescript-eslint/parser": "^7.16.1",
		"eslint": "^8.57.0",
		"eslint-config-airbnb-typescript": "^18.0.0",
		"eslint-import-resolver-typescript": "^3.6.3",
		"eslint-plugin-import": "^2.31.0",
		"jest": "^29.7.0",
		"nodemon": "^3.1.7",
		"prisma": "^6.7.0",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	}
}
